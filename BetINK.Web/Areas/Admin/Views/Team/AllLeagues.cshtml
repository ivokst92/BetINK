@model IEnumerable<SelectItemLeague>
@{
    ViewData["Title"] = "Choose League";
}
<h2>@ViewData["Title"]</h2>
@if (!(Model.Count() == 0))
{

    @foreach (var league in Model)
    {
        <a asp-controller="Team" asp-action="All" asp-route-leagueId="@league.Id" class="spaceLeft btn btn-sm btn-primary">@league.Name</a>
    }
}
else
{
    <h3>You need to create a league first!</h3>
}
